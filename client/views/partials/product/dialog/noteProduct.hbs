<el-dialog :title="title" :visible.sync="dialogFormNoteProduct" class="form-label">
    <el-form :label-position="labelposition" :model="noteForm" :rules="noteValidate" ref="noteForm"label-width="120px" class="demo-ruleForm">
        <el-form-item label="Tiêu Đề Ghi Chú" prop="title">
            <el-input v-model="noteForm.title"></el-input>
        </el-form-item>
        <el-form-item label="Nội Dung Ghi Chú" prop="content">
            <el-input v-model="noteForm.content"></el-input>
        </el-form-item>
        <el-form-item label="Trạng Thái" prop="status">
            <el-radio-group v-model="noteForm.status">
                <el-radio-button :label="item.label" :name="item.name" v-for="item in listData.radioStatus"></el-radio-button>
            </el-radio-group>
        </el-form-item>
        <el-form-item>
            <el-time-picker class="w-65" is-range arrow-control v-model="noteForm.date" range-separator="To" start-placeholder="Start time"
                end-placeholder="End time">
            </el-time-picker>
        </el-form-item>
    </el-form>
    <span slot="footer" class="dialog-footer">
        <el-button @click="dialogVisible = false">Huỷ Bỏ</el-button>
        <el-button type="primary" @click="createNoteProduct('noteForm')">Xác Nhận</el-button>
    </span>
</el-dialog>
